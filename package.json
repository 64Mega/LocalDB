{
    "name": "localdb",
    "description": "Simple wrapper around LocalStorage, providing some DB-like features",
    "authors": "Daniel Lawrence",
    "version": "1.0.4",
    "private": true,
    "main": "./dist/index.js",
    "type": "module",
    "module": "./dist/index.esm.js",
    "targets": {
        "main": {
            "outputFormat": "esmodule"
        }
    },
    "types": "./src/index.d.ts",
    "files": [
        "dist/**/*.js",
        "src/**/*.ts"
    ],
    "scripts": {
        "build": "parcel build src/index.ts"
    },
    "devDependencies": {
        "@parcel/packager-ts": "^2.0.0-beta.3.1",
        "@parcel/transformer-typescript-types": "^2.0.0-beta.3.1",
        "concurrently": "^6.2.0",
        "parcel": "^2.0.0-beta.3.1",
        "typescript": "^4.3.2"
    }
}
